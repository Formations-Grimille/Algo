ALGO  Notes
CONSTANTES 
  N <- 15, M <- 3 : entier
VARIABLES 
  note : tableau[N, M] d'entier
  i, j : entier
DÉBUT
  //Remplissage du tableau note
  POUR i ALLANT DE 0 À N-1 FAIRE
    POUR j ALLANT DE 0 À M-1 FAIRE
       ÉCRIRE("Entrer la note de l'étudiant", i+1, " dans la matière ", j+1)
       LIRE(note[i, j])
    FINPOUR
  FINPOUR
FIN